{
  "name": "backend-graphql-impch",
  "version": "1.4.5",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "db:up": "docker-compose up -d postgres",
    "db:down": "docker-compose down",
    "db:logs": "docker-compose logs -f postgres",
    "db:reset": "docker-compose down -v && docker-compose up -d postgres",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:migrate:status": "sequelize-cli db:migrate:status",
    "bump-patch": "npm version patch -m \"Bump version to %s\" && git push && git push --tags",
    "bump-minor": "npm version minor -m \"Bump version to %s\" && git push && git push --tags",
    "bump-major": "npm version major -m \"Bump version to %s\" && git push && git push --tags"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@apollo/utils.keyvaluecache": "^4.0.0",
    "apollo-server-express": "^3.13.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "graphql": "^16.12.0",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^9.0.3",
    "moment-timezone": "^0.6.0",
    "pg": "^8.17.2",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.10",
    "@types/pg": "^8.16.0",
    "nodemon": "^3.1.11",
    "sequelize-cli": "^6.6.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
